<%@ Page Language="C#" AutoEventWireup="true" CodeFile="Check_Message.aspx.cs" Inherits="Code_MessageBox_Check_Message" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <title>信息详情</title>
    <link href="../../Bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <style type="text/css">
        .td {
            width:20%;
            background-color:#f1f1f1;
            text-align:right;
        }
    </style>
</head>
<body>
    <form id="form1" runat="server">
    <div style="width:100%">
        <table style="margin:auto;margin-top:60px;width:500px" class="table table-bordered">
            <tr>
                <td class="td">接收者：</td><td><asp:Label ID="studentnum" runat="server"></asp:Label> </td>
            </tr>
            <tr>
                <td class="td">消息时间：</td><td><asp:Label ID="Create_Time" runat="server"></asp:Label> </td>
            </tr>
            <tr>
                <td class="td">消息内容：</td><td><asp:TextBox TextMode="MultiLine" ReadOnly="true" Width="99%" BorderStyle="None" ID="Contents" runat="server" style="resize:none"></asp:TextBox></td>
            </tr>
        </table>
       
    </div>
    </form>
</body>
</html>
