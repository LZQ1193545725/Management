<%@ Page Language="C#" AutoEventWireup="true" CodeFile="Check_Metting.aspx.cs" Inherits="Code_Work_Message_Check_Metting" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <title>会议记录详情</title>
    <link href="../../Bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <script src="../../JavaScript/JSMethod.js"></script>
    <script src="../../JavaScript/js/laydate/laydate.js"></script>
    <style type="text/css">
        .td {
            width:20%;
            background-color:#f1f1f1;
            text-align:right;
        }
        
    </style>
</head>
<body>
    <form id="form1" runat="server">
    <div style="width:100%">
       <table style="margin:auto;margin-top:60px;width:500px" class="table table-bordered">
           <tr>
               <td class="td">会议主题：</td>
               <td><asp:Label ID="Metting_Theme" runat="server"></asp:Label> </td>
           </tr>
           <tr>
               <td class="td">会议时间：</td>
               <td>
                   <asp:Label ID="Work_Message_Time" runat="server" ></asp:Label>
               </td>
           </tr>
           <tr>
               <td class="td">发布者：</td>
               <td>
                   <asp:Label ID="Promulgator" runat="server"></asp:Label>
               </td>
           </tr>
           <tr>
               <td class="td">会议内容：</td>
               <td style="height:100px">
                   <asp:Label ID="Work_Content" runat="server"></asp:Label>
               </td>
           </tr>
       </table>
    </div>
    </form>
</body>
</html>
